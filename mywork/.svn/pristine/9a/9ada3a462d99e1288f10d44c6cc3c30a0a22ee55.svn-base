<!-- 登入框(網頁版) -->
<div id="loginModal" style="display:none;padding-left: 88px;" class="modal fade pr-0"  aria-labelledby="loginModal">
	<div class="modal-dialog modal-dialog-m">

		<!--<div class="homepage_login_cancel"><img src="__CSS__/images/homepage_login_cancel.png" alt=""><span class="homepage_login_span">&times;</span></div>  -->
		<div class="homepage_login_girl"><img style="max-width:100%;" src="__CSS__/images/index-popups-gril.png" alt=""></div>
		<div class="modal-content homepage_login_bg">

			<div class="modal-header p-0" style="border-bottom: 0;">
				<div class="homepage_login_cancel"><img src="__CSS__/images/homepage_login_cancel.png" alt="">
					<button type="button" id="closeModalBtn" class="close homepage_login_span" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				</div>
			</div>
			<!-- Modal Body -->
			<div class="modal-body modal-body-m">
				<ul class="nav nav-tabs nav-justified">
					<li class="nav-item"><a class="nav-link bookmark_type active" href="#home" data-toggle="tab">{$Think.lang.LoginGame}</a></li>
					<li class="nav-item"><a class="nav-link bookmark_type" href="#profile" data-toggle="tab">{$Think.lang.LoginAPPDown}</a></li>

				</ul>

				<div class="tab-content pt-35 pb-0">
					<div class="tab-pane active" id="home">
						<form class="mb-0" id="loginForm" name="loginForm" method="post" action='__APP__/MemberIndex/formLogin' autocomplete="off" onsubmit="return loginFunction()">
							<div class="row">
								<div class="col-md-10 m-auto">
									<div class="clearfix">

										<!-- Email -->
										<div class="form-group">
											<div class="fancy-form">
												<i class="fa fa-user txt-white"></i>
												<input type="text" name='account' id="loginFormAccount" class="form-control login-input" placeholder="{$Think.lang.loginMenu1}">
											</div>
										</div>

										<!-- Password -->
										<div class="form-group">
											<div class="fancy-form">
												<i class="fa fa-lock txt-white"></i>
												<input type="password" name='password' id="loginFormPassword" class="form-control login-input" placeholder="{$Think.lang.loginMenu2}">
											</div>
										</div>

									</div>

									<div class="row mb-10">
										<div class="col-md-5 col-sm-5 col-5 m-auto">
											<label class="checkbox m-0" style="vertical-align: sub;">
												<input type="checkbox" id="checkbox1" value="1" checked="checked">
												<i></i>
											</label>
											<span class="pr-10 txt-white" onclick="openmodal('MemberConditionModal')" style="cursor: pointer;">{$Think.lang.LoginAgree}</span>
										</div>
									</div>
								</div>
								<div class="col-md-12"><button id="loginBtn" type="submit" class="btn btn-primary login-btn">{$Think.lang.LoginImmediately}</button></div>
							</div>
						</form>

					</div>
					<div class="tab-pane fade" id="profile">
						<div class="row game-row appDownload">
							<div class="col-8 col-sm-8 mb-0 m-auto text-center pb-30">
								<a href="#">
										<img src="__CSS__/images/{$langStr}/promotions/btn.png" style="width: 100%; height: auto;" >
										<!--<i class="icomoon icon-app-phone-2 icon-app-pc-2-style"></i>-->
								</a>
							</div>
						</div>

					</div>
				</div>
			</div>
			<!-- Modal Footer -->
			<div class="modal-footer modal-footer-m">
				<a class="txt-white loading" href="__APP__/OnlineService/index">{$Think.lang.ContactService}</a>
				<span>|</span>
				<a class="txt-white loading" href="__APP__/ForgetPage/index.html">{$Think.lang.LoginForgPwd}</a>
			</div>

		</div>
	</div>
</div>
<!-- 註冊框(網頁版) -->
<div id="registModal" style="display:none;padding-left: 88px;" class="modal fade pr-0"  aria-labelledby="registModal" data-backdrop="true">
	<div class="modal-dialog modal-dialog-m">

		<!-- <div class="homepage_login_cancel"><img src="__CSS__/images/homepage_login_cancel.png" alt=""><span class="homepage_login_span">&times;</span></div> -->
		<div class="homepage_login_girl"><img style="max-width:100%;" src="__CSS__/images/index-popups-gril.png" alt=""></div>
		<div class="modal-content homepage_login_bg">

			<!-- Modal Header -->
			<div class="modal-header p-0" style="border-bottom: 0;">
				<div class="homepage_login_cancel"><img src="__CSS__/images/homepage_login_cancel.png" alt="">
					<button type="button" id="closeModalBtn" class="close homepage_login_span" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				</div>
			</div>
			<!-- Modal Body -->
			<div class="modal-body modal-body-m">
				<ul class="nav nav-tabs nav-justified">
					<li class="nav-item"><a class="nav-link bookmark_type active" href="#regist" data-toggle="tab">{$Think.lang.loginMenu3}</a></li>
					<li class="nav-item"><a class="nav-link bookmark_type" href="#profile1" data-toggle="tab">{$Think.lang.LoginAPPDown}</a></li>

				</ul>

				<div class="tab-content pt-35 pb-0">
					<div class="tab-pane active" id="regist">
						<div class="row">
							<div class="col-md-10 m-auto">

								<!-- login form -->
								<form class="mb-0">
									<div class="clearfix">

										<!-- Phone -->
										<div class="form-group">
											<label class="txt-white" id="addPhoneLabel" style="display:none;">{$Think.lang.SamrtPhne}</label>
											<div class="fancy-form">
												<i id="phoneIcon" class="fa fa-mobile txt-white regist-icon"></i>
												<input type="text" id="addPhone" class="form-control login-input" placeholder="{$Think.lang.LoginEnterPhone}">
												<span id="checkPhone" class="invalid-feedback">{$Think.lang.LoginPhoneLeng}</span>
												<span class="valid-feedback">{$Think.lang.LoginOKPhone}</span>
											</div>
										</div>


										<!-- 帳號 -->
										<div id="addAccountDiv" class="form-group" style="display:none;">
											<label class="txt-white" id="addAccountLabel">{$Think.lang.Account}</label>
											<div class="fancy-form">
												<i class="fa fa-mobile txt-white regist-icon"></i>
												<input type="text" id="addAccount" name="account" class="form-control login-input" >
											</div>
										</div>


										<!-- Password -->
										<div id="addPasswordDiv" class="form-group">
											<label class="txt-white" id="addPasswordLabel"></label>
											<div class="fancy-form">
												<i class="fa fa-lock txt-white"></i>
												<input type="password" id="addPassword" name="password" class="form-control login-input" placeholder="{$Think.lang.LoginEnterPwd}">
											</div>
										</div>

										<!-- Password -->
										<div id="addCheckPasswordDiv" class="form-group">
											<div class="fancy-form">
												<i class="fa fa-lock txt-white"></i>
												<input type="password" id="addCheckPassword" class="form-control login-input" placeholder="{$Think.lang.LoginDoublPwd}">
											</div>
										</div>

										<!-- 驗證碼 -->
										<div id="addVerificationCodeDiv" class="form-group" style="display:none;">
											<div class="fancy-form">
												<i class="fa fa-lock txt-white"></i>
												<input type="text" id="addVerificationCode" class="form-control login-input" placeholder="{$Think.lang.Verification}">
											</div>
										</div>

									</div>

									<div class="row mb-10">
										<div class="col-md-5 col-sm-5 col-5 m-auto">
											<!-- Inform Tip -->
											<label class="checkbox m-0" style="vertical-align: sub;">
												<input type="checkbox" id="checkbox2" value="1" checked="checked">
												<i></i>
											</label>
											<span class="pr-10 txt-white" onclick="openmodal('MemberConditionModal')" style="cursor: pointer;">{$Think.lang.LoginAgree}</span>
										</div>
									</div>
								</form>
							</div>
							<div class="col-md-12">
								<button id="registBtn" type="button" onclick="regist();"class="btn btn-primary login-btn">{$Think.lang.LoginSignUpNow}</button>
								<button id="sendOutBtn" type="button" onclick="sendOut();"class="btn btn-primary login-btn" style="display:none;">{$Think.lang.LoginSubmitNow}</button>
								<button id="finishBtn" type="button" onclick="self.location.href='__APP__/MemberIndex/index'" class="btn btn-primary login-btn loading" style="display:none;">{$Think.lang.LoginComplete}</button>
							</div>
						</div>


					</div>
					<div class="tab-pane fade" id="profile1">
						<div class="row game-row appDownload">
							<div class="col-6 col-sm-5 mb-0 m-auto">
								<div class="icon-bg-style-pc">
									<a href="#">
										<i class="icomoon icon-app-phone-2 icon-app-pc-2-style"></i>
									</a>
								</div>
							</div>
						</div>
						<div class="row pb-35 pt-10 appDownload">
							<div class="col-6 col-sm-5 mb-0 m-auto text-center">
								<a href="#">
									<label class="fs-25 txt-white">{$Think.lang.LoginAPPDown}</label>
								</a>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- Modal Footer -->
			<div class="modal-footer modal-footer-m">
				<a class="txt-white loading" href="__APP__/OnlineService/index">{$Think.lang.ContactService}</a>
				<span>|</span>
				<a class="txt-white loading" href="__APP__/ForgetPage/index.html">{$Think.lang.LoginForgPwd}</a>
			</div>

		</div>
	</div>
</div>