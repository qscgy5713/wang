					                      <th>贈點</th>
				                      </tr>
					                </thead>
									<tbody>
										<VOLIST name="agentArray" id="vo" empty="$agentEmpty">
											<tr>
												<td>{$i}</td>
												<td>
													<input class="btn btn-pink f-s-13 p-5" style="width: 80px;" type='button' value='{$vo.agentAccount}' onclick="formAgent('{$vo.agentAccount}','{$vo.agentTree}')">
												</td>
												<td>{$vo.ratioRemark}</td>
												<td>{$vo.sumBettingMoney}</td>
												<td style='color: {$vo['sumSurplus']>=0?'green':'red'}'>{$vo.sumSurplus}</td>
												<td>{$vo.sumEffective}</td>
												<td>{$vo.sumBetNumber}</td>
												<td>{$vo.sumGivenMoney}</td>
											</tr>
										</VOLIST>
									</tbody>
								<ELSE/>
									<thead>
								   	 <tr style="background-color: rgba(255, 45, 85, 0.3607843137254902);">
									   	 <td colspan="3" class="f-w-600">總計</td>
									   	 <th>{$agentTotalBettingMoney}</th>
										 <th>{$agentTotalSurplus}</th>
										 <th>{$agentTotalEffective}</th>
										 <th>{$agentTotalBetNumber}</th>
										 <th>{$agentTotalGiven}</th>
										 <th>{$agentTotalHandlingFee}</th>
										 <th>{$agentTotalUnderMoney}</th>
										 <!--<th></th>
										 <th>{$agentTotalRatioMoney}</th>
										 <th>{$agentTotalTopMoney}</th> -->
								   	 </tr>
				                     <tr style="background-color: rgba(255, 45, 85, 0.3607843137254902);">
					                      <th>序</th>
					                      <th>代理</th>
					                      <th>佔成</th>
					                      <th>投注金額</th>
					                      <th>輸贏金額</th>
					                      <th>有效注額</th>
					                      <th>注單數量</th>
					                      <th>贈點</th>
					                      <th>手續費</th>
					                      <th>交收金額</th>
					                      <!--<th>佔成</th>
					                      <th>佔成金額</th>
					                      <th>拆帳金額</th>-->
				                      </tr>
					                </thead>
									<tbody>
										<VOLIST name="agentArray" id="vo" empty="$agentEmpty">
											<tr>
												<td>{$i}</td>
												<td>
													<input class="btn btn-pink f-s-13 p-5" style="width: 80px;" type='button' value='{$vo.agentAccount}' onclick="formAgent('{$vo.agentAccount}','{$vo.agentTree}')">
												</td>
												<td>{$vo.underRatio} 備:{$vo.ratioRemark}</td>
												<td>{$vo.sumBettingMoney}</td>
												<td style='color: {$vo['sumSurplus']>=0?'green':'red'}'>{$vo.sumSurplus}</td>
												<td>{$vo.sumEffective}</td>
