<body class="pace-top">
	
	<div id="page-container" class="fade">
		<!-- begin error -->
		<div class="error">
			<present name="error" >
				<div class="error-code m-b-10"><i class="fas fa-lg fa-fw m-r-10 fa-minus-circle" style="color:red"></i></div>
			</present>
			<present name="message" >
				<div class="error-code m-b-10 text-blue"><i class="fas fa-lg fa-fw m-r-10 fa-check-circle"></i></div>
			</present>
			
			
			<div class="error-content">
				<present name="error" >
					<div class="error-message" style="color:red">{$error}</div>
		      	</present>
		      	<present name="message" >
		      		<div class="error-message text-blue">{$message}</div>
		      	</present>
				<div>
					<a onclick="javascript:location.href='{$jumpUrl}'" class="btn btn-primary p-20 text-white m-t-20 f-s-35">返回</a>
				</div>
			</div>
		</div>
		<!-- end error -->
	</div>
	
