}
.loader-bg {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 9999;
    overflow: hidden;
    position: fixed;
    background: hsl(0, 0%, 0%);
    opacity: 0.8;
}
.icon-size:hover, .sort-icon:hover {
    color: #FF3B30;
}
.icon-size {
    margin-right: 4px!important;
