	display: inline-flex;
}
.mq-height{
	float: left;
    padding: 10px 20px;
    height: 50px;
    width: 77%;
    font-size: 14px;
    text-decoration: none;
}
@media screen and (max-width: 520px){
	.mq-height{
		width: 100%;
	}
	.time-btn-rwd{
    	margin-bottom: 10px !important;
    }
    .time-input-rwd{
