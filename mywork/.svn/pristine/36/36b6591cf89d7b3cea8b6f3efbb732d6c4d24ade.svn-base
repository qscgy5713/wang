class ApiAction extends CommonAction {
    
    protected function _initialize() {
        // 執行父類初始化
        parent::_initialize();
    }
    
    public function modelGetApiData($data,&$err = ""){
        if(empty($data['operValue']) && $data['operValue'] !== 0){
            $err = L(strtoupper('ERROR_' . __CLASS__ . '_' . __FUNCTION__ . '_01'));
            return false;
        }
        if(empty($data['game_name']) && $data['game_name'] !== 0){
            $err = L(strtoupper('ERROR_' . __CLASS__ . '_' . __FUNCTION__ . '_02'));
            return false;
        }
        $operValue= $data["operValue"];
        $gameName = $data["game_name"];
        
        
        switch($operValue){
            case 3://取得遊戲餘額
                $data = array(
                    "game_name" => $gameName
                );
                $return = $this->getMemberBalance($data,$err);
                if($return === false){
                    return false;
                }
                return $return;
                break;
            case 4://對遊戲下點
                $money = $data["money"];
                $data = array(
                    "game_name" => $gameName,
                    "money" => $money
                );
                $return = $this->withdrawMemberBalance($data,$err);
                if($return === false){
                    return false;
                }
                return $return;
                break;
            case 5://對遊戲上點
                $money = $data["money"];
                $data = array(
                    "game_name" => $gameName,
                    "money" => $money
                );
                $return = $this->rechargeMemberBalance($data,$err);
                if($return === false){
                    return false;
                }
                return $return;
                break;
        }
    }
    /**
