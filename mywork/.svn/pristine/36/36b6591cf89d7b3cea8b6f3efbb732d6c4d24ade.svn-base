        $_SESSION['Api']['functionName'] = strtoupper(__FUNCTION__);
        
        $return = $this->getMemberInfoData($data,$err);
        if($return === false){
            return false;
        }
        
        $gameAccount = $return["gameAccount"];
        $gamePassword = $return["gamePassword"];
        
        $gameLink = "";
        $gameModel = D('Game'.$gameName);//gameName 必須傳入 第一個字母大寫 後面小寫
        switch($gameName){
            case "Dg"://夢幻真人
                $is_mobile = "n";//手機板?(非必須)
                if(isMobile()){
                    $is_mobile = "y";
                }
                $is_apk = "n";//用APP進行遊戲?(非必須)
                $lang_code = "tw";//語系代碼(非必須)
                
                $data = array(
                    "user_name" => $gameAccount,
                    "password" => $gamePassword,
                    'is_mobile' => $is_mobile,
                    'is_apk' => $is_apk,
                    'lang_code' => $lang_code
                );
                break;
            case "Sa"://沙龍真人
                $currency_type = 'TWD';//貨幣代碼
                
                $data = array(
                    'user_name' => $gameAccount,
                    'currency_type' => $currency_type,
                );
                break;
            case "Ab"://歐博真人
                $data = array(
                    "user_name" => $gameAccount,
                    "password" => $gamePassword
                );
                break;
            case "Sb"://鑫寶體育
                $is_mobile = isMobile();
                $token = '';
                $data = array(
                    'user_name' => $gameAccount,
                    'password' => $gamePassword,
                    'is_mobile' => $is_mobile,
                    'token' => $token
                );
                break;
            case "Sp"://SP體育
                $data = array(
                    "user_name" => $gameAccount,
                    "password" => $gamePassword
                );
                break;
            case "Pa"://平博體育
                $data = array(
                    'user_name' => $gameAccount
                );
                break;
            case "Bj"://北京賽車
                $data = array(
                    'account' => $gameAccount
                );
                break;
            case "Xbs"://六合彩
                $data = array(
                    'user_name' => $gameAccount,
                    'password' => $gamePassword
                );
                break;
            case "Bg"://賓果
                $data = array(
                'user_name' => $gameAccount
                );
                break;
            case "Rtg"://RTG電子
                $data = array(
                    'account' => $gameAccount,
                );
                break;
            case "Bng"://博狗
                $data = array(
                    'user_name' => $gameAccount,
                    'game_code' => $data['game_code']
                );
                break;
            case "Qt"://Qt電子
                $data = array(
                    'user_name' => $gameAccount,
                    'game_id' => $data['game_id'],
                    'is_mobile'=> $data['is_mobile']
                );
                break;
            case "Uc8"://Uc8手機電子
                $data = array(
                    'user_name' => $gameAccount,
                    'is_mobile'=> $data['is_mobile']
                );
                break;
            case "Gg"://Gg捕魚
                $data = array(
                    'user_name' => $gameAccount,
                    'password' => $gamePassword,
                    'game_code' => $data['game_code']
                );
                break;
            case "Ae"://泛亞電競
                $data = array(
                    'account' => $gameAccount,
                );
                break;
            case "Og"://泛亞電競
                $data = array(
                    'account' => $gameAccount,
                    'is_mobile' => isMobile(),
                );
                break;
            default://未傳入館別
                $data = array(
                    'account' => $gameAccount,
                    'is_mobile' => isMobile(),
                );
                break;
        }
        $return = $gameModel->loginMember($data, $err);
        if($return === false){
            //錯誤API CURL ERR LOG 新增
            $errData = array(
                "game_code"=>$gameName,
                "member_id"=>$memberId,
                "err_function"=>__CLASS__.'_'.__FUNCTION__,
                "err_message"=>$err,
                "err_return"=>'false',
                "json_data"=>json_encode($data),
            );
            $this->addApiCurlErr($errData);
            return false;
        }
        $gameLink= $return;
        
        return $gameLink;
    }
