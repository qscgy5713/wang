 <script>
 //$(document).ready(function() { $("#e1").select2(); }); //有搜尋功能的select
 
 $(document).ready(function(){
	var bankCode = '{$bankCode}';
	//alert(bankCode);
	$("#e1").find("option[value = "+bankCode+"]").attr("selected",true);
});
//確認託售
 $("button[name='btnok']").click(function(){
	 var addWithdraw = $("input[name='addWithdraw']").val(); //託售點數
	 var memberWallet = {$memberWallet};//電子錢包
	 
	 addWithdraw = $.trim(addWithdraw);
	 memberWallet = $.trim(memberWallet);
	 
	 if (memberWallet < addWithdraw){
		 alert("餘額不足");
		 return false;
	 }
	 if(addWithdraw < 1000){
		 alert("託售最低限額為1000點");
		 return false;
	 }
 })
 

</script>


