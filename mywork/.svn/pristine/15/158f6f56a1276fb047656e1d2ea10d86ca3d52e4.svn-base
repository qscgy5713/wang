<include file="Common:header"/>

<div id="page-loader" class="fade show"><span class="spinner"></span></div>
<div id="page-container" class="fade page-sidebar-fixed page-header-fixed">
	<include file="Common:menu"/>
	<include file="Common:menu2"/>
	<div class="sidebar-bg"></div>
	
	<div id="content" class="content">
		<div class="form-group row m-b-15">
			<ol class="breadcrumb pull-left">
				<VOLIST name="loginArray" id="vo">
					<li class="breadcrumb-item">
						<a onclick="formAgent('{$vo.agentAccount}','{$vo.agentTree}')" href="#" >{$vo.agentAccount}</a>
					</li>
				</VOLIST>
			</ol>
		</div>
		
		
		<form class="div-style" id="formInquire" method="post" action='__APP__/FirstDepositwithdraw/index'>
			<div class="form-inline m-b-10">
				<input id="agentTree" type='hidden' name='agentTree' value='{$comeAgentTree}'>
				<input id="agentAccount" type='hidden' name='agentAccount' value='{$comeAgentAccount}'>
				<input class="btn btn-info f-s-13 p-5 m-r-5 time-btn-rwd" type="button" value="本日" onClick="formTime('regist','{$today} 00:00:00','{$today} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-10 time-btn-rwd" type="button" value="昨日" onClick="formTime('regist','{$yesterday} 00:00:00','{$yesterday} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-5 time-btn-rwd" type="button" value="本週" onClick="formTime('regist','{$thisWeekA} 00:00:00','{$thisWeekB} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-10 time-btn-rwd" type="button" value="上週" onClick="formTime('regist','{$lastWeekA} 00:00:00','{$lastWeekB} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-5 time-btn-rwd" type="button" value="本月" onClick="formTime('regist','{$thisMonthA} 00:00:00','{$thisMonthB} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-20 time-btn-rwd" type="button" value="上月" onClick="formTime('regist','{$lastMonthA} 00:00:00','{$lastMonthB} 23:59:59')"/>
				<div class="form-group m-r-10 time-input-rwd">
					<input id="registStartTime" name="registStartTime" type="text" class="form-control div-style" style="width: 175px;" value="{$registStartTime}"/>
				</div>
				<div class="form-group m-r-10 time-input-rwd"><label class="m-b-0">至</label></div>
				<div class="form-group m-r-10 time-input-rwd">
				    <input id="registEndTime"   name="registEndTime" type="text" class="form-control div-style" style="width: 175px;" value="{$registEndTime}"/>
				</div>
			    <input class="btn btn-info p-5 m-r-5" type='button' value='清除' onClick="clearTime('regist');">
			</div>
			
			<div class="form-inline m-b-20">
				<input class="btn btn-info f-s-13 p-5 m-r-5 time-btn-rwd" type="button" value="本日" onClick="formTime('profit','{$today} 00:00:00','{$today} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-10 time-btn-rwd" type="button" value="昨日" onClick="formTime('profit','{$yesterday} 00:00:00','{$yesterday} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-5 time-btn-rwd" type="button" value="本週" onClick="formTime('profit','{$thisWeekA} 00:00:00','{$thisWeekB} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-10 time-btn-rwd" type="button" value="上週" onClick="formTime('profit','{$lastWeekA} 00:00:00','{$lastWeekB} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-5 time-btn-rwd" type="button" value="本月" onClick="formTime('profit','{$thisMonthA} 00:00:00','{$thisMonthB} 23:59:59')"/>
				<input class="btn btn-info f-s-13 p-5 m-r-20 time-btn-rwd" type="button" value="上月" onClick="formTime('profit','{$lastMonthA} 00:00:00','{$lastMonthB} 23:59:59')"/>
				<div class="form-group m-r-10 time-input-rwd">
					<input id="profitStartTime" name="profitStartTime" type="text" class="form-control div-style" style="width: 175px;" value="{$profitStartTime}"/>
				</div>
				<div class="form-group m-r-10 time-input-rwd"><label class="m-b-0">至</label></div>
				<div class="form-group m-r-10 time-input-rwd">
					<input id="profitEndTime"   name="profitEndTime" type="text" class="form-control div-style"style="width: 175px;" value="{$profitEndTime}"/>
				</div>
				<input class="btn btn-info p-5 m-r-5" type='button' value='清除' onClick="clearTime('profit');">
				<input class="btn btn-info p-5 m-r-5" type='submit' value='查詢'>
				
			</div>
		</form>
		
		
		<div class="row">
			<div class="col-lg-12 ui-sortable">
				<!-- begin panel -->
				<div class="panel panel-inverse" data-sortable-id="table-basic-1">
					<!-- begin panel-heading -->
					<div class="panel-heading ui-sortable-handle" data-click="panel-collapse"> 
						<div class="panel-heading-btn">
							<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
						</div>
						<h4 class="panel-title f-s-15">代理帳務</h4>
					</div>
					<!-- end panel-heading -->
					
					<!-- begin panel-body -->
					<div class="panel-body">
						<!-- begin table-responsive -->
						<div class="table-responsive">
							<table id="agentTable" class="table table-striped m-b-0">
							   <thead>
							   	 <tr><td colspan="7" class="text-center">合計</td><td colspan="2">{$agentTotalMoney}</td></tr>
								<tr><td colspan="7" class="text-center">總儲值</td><td colspan="2">{$agentTotalRechargeMoney}</td></tr>
								<tr><td colspan="7" class="text-center">總託售</td><td colspan="2">{$agentTotalWithdrawMoney}</td></tr>
								<tr><td colspan="7" class="text-center">筆數</td><td colspan="2">{$agentTotalNumber}</td></tr>
								<tr><td colspan="7" class="text-center">註冊總人數</td><td colspan="2">{$agentTotalRegist}</td></tr>
			                      <tr>
				                      <th>序</th>
				                      <th>代理</th>
				                      <th>註冊人數</th>
				                      <th>儲值</th>
				                      <th>託售</th>
				                      <th>筆數</th>
				                      <th>點數</th>
			                      </tr>
				                </thead>
								<tbody>
									<VOLIST name="agentArray" id="vo" empty="$agentEmpty">
										<tr>
											<td>{$i}</td>
											<td>
												<input class="btn btn-info f-s-13 p-5" type='button' value='{$vo.agentAccount}' onclick="formAgent('{$vo.agentAccount}','{$vo.agentTree}')">
											</td>
											<td>{$vo.registNumber}</td>
											<td>{$vo.rechargeMoney}</td>
											<td>{$vo.withdrawMoney}</td>
											<td>{$vo.sumNumber}</td>
											<td>{$vo.sumMoney}</td>
										</tr>
									</VOLIST>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
		
		<div class="row">
			<div class="col-lg-12 ui-sortable">
				<!-- begin panel -->
				<div class="panel panel-inverse" data-sortable-id="table-basic-1">
					<!-- begin panel-heading -->
					<div class="panel-heading ui-sortable-handle" data-click="panel-collapse"> 
						<div class="panel-heading-btn">
							<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
						</div>
						<h4 class="panel-title f-s-15">{$comeAgentAccount} 會員帳務</h4>
					</div>
					<!-- end panel-heading -->
					
					<!-- begin panel-body -->
					<div class="panel-body">
						<!-- begin table-responsive -->
						<div class="table-responsive">
							<table id="memberTable" class="table table-striped m-b-0">
							   <thead>
							   	 <tr><td colspan="7" class="text-center">合計</td><td colspan="2">{$memberTotalMoney}</td></tr>
								<tr><td colspan="7" class="text-center">總儲值</td><td colspan="2">{$memberTotalRechargeMoney}</td></tr>
								<tr><td colspan="7" class="text-center">總託售</td><td colspan="2">{$memberTotalWithdrawMoney}</td></tr>
								<tr><td colspan="7" class="text-center">筆數</td><td colspan="2">{$memberTotalNumber}</td></tr>
								<tr><td colspan="7" class="text-center">註冊總人數</td><td colspan="2">{$memberTotalRegist}</td></tr>
			                      <tr>
				                      <th>序</th>
				                      <th>會員帳號</th>
				                      <th>註冊時間</th>
				                      <th>儲值</th>
				                      <th>託售</th>
				                      <th>筆數</th>
				                      <th>點數</th>
			                      </tr>
				                </thead>
								<tbody>
									<VOLIST name="memberArray" id="vo" empty="$memberEmpty">
										<tr>
											<td>{$i}</td>
											<td>{$key}</td>
											<td>{$vo.memberCreateTime}</td>
											<td>{$vo.rechargeMoney}</td>
											<td>{$vo.withdrawMoney}</td>
											<td>{$vo.sumNumber}</td>
											<td>{$vo.sumMoney}</td>
										</tr>
									</VOLIST>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<a href="javascript:;" class="btn btn-icon btn-circle btn-success btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
</div>
<include file="Common:footer"/>

<script>
$(function(){
	$( "#registStartTime" ).datetimepicker({
    	dateFormat: "yy-mm-dd",
    	timeFormat: "HH:mm:ss",
        maxDate: "+0",
        changeMonth: true,
        numberOfMonths: 1,
        // showButtonPanel: true,
        onClose: function( selectedDate ) {
            $( "#endtime" ).datepicker( "option", "minDate", selectedDate );
        }
    });
    $( "#registEndTime" ).datetimepicker({
    	dateFormat: "yy-mm-dd",
    	timeFormat: "HH:mm:ss",
        maxDate: "+0",
        changeMonth: true,
        numberOfMonths: 1,
        // showButtonPanel: true,
        onClose: function( selectedDate ) {
            $( "#begintime" ).datepicker( "option", "maxDate", selectedDate );
        }
    });
    
    $( "#profitStartTime" ).datetimepicker({
    	dateFormat: "yy-mm-dd",
    	timeFormat: "HH:mm:ss",
        maxDate: "+0",
        changeMonth: true,
        numberOfMonths: 1,
        // showButtonPanel: true,
        onClose: function( selectedDate ) {
            $( "#endtime" ).datepicker( "option", "minDate", selectedDate );
        }
    });
    $( "#profitEndTime" ).datetimepicker({
    	dateFormat: "yy-mm-dd",
    	timeFormat: "HH:mm:ss",
        maxDate: "+0",
        changeMonth: true,
        numberOfMonths: 1,
        // showButtonPanel: true,
        onClose: function( selectedDate ) {
            $( "#begintime" ).datepicker( "option", "maxDate", selectedDate );
        }
    });
    
});
function formTime(idName,startTime,endTime){
	$("#"+idName+"StartTime").val(startTime);
	$("#"+idName+"EndTime").val(endTime);
	$("#formInquire").submit();
}
function formAgent(agentAccount,agentTree){
	$("#agentAccount").val(agentAccount);
	$("#agentTree").val(agentTree);
	$("#formInquire").submit();
}
function clearTime(idName){
	$("#"+idName+"StartTime").val('');
	$("#"+idName+"EndTime").val('');
}
</script>