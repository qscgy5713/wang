.bghover:hover{
	color: #fff;
    background-color: rgba(0,0,0,0.5);
    background-image: none;
    border-color: #fff;
}
.aservise{
	text-decoration:none!important;
}
.bservise{
	color:#fff;
}
.bservise:hover{
	background-color: #080808 !important;
	cursor:pointer;
}
.white-bg {
	
	height:60px;
    position: relative;
    left: 0px;
    top:-5px;
    float: left;
    width: 200px;
    padding: 0px 0;
    font-size: 20px;
    line-height: 63px;
    letter-spacing: 0px;
    text-align: center;
    background-color: #1a1a1a;
    color: #fff;
    -webkit-clip-path: polygon(0 0, 100% 0%, 84% 100%, 0% 100%);
    clip-path: polygon(0 0, 100% 0%, 84% 100%, 0% 100%);
}
.white-bg2{
	height:60px;
	position: relative;
    left: -28px;
    top:-5px;
    float: left;
    margin:0;
    padding:0;
	width: 130px;
	
	background-color: #1a1a1a;
    -webkit-clip-path: polygon(25% 0%, 31% 0, 7% 100%, 0% 100%);
    clip-path: polygon(25% 0%, 31% 0, 7% 100%, 0% 100%);
}

.width-100{
	width:100%!important;
	margin:0;
}
.m-tb-0{
    height:50px!important;
}

.container-fluid{
	width: 100%;
    padding-right: 300px!important;
