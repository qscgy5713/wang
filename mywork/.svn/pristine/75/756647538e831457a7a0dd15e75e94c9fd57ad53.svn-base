            'note_processMain' => 'processMain',
            'note_processRemark1' => 'processRemark1',
            'note_processRemark2' => 'processRemark2',
            'note_processModifyTime' => 'processModifyTime',
        );

        foreach($return as $key => $value){
            foreach ($FieldChange as $key1 => $value1) {
                //欄位特別處理
                switch ($key1) {
                    case 'mail_ip': //IP轉換
                        $showStr = get_ip_inet_ntop($value[$key1]);//IP轉換可讀格式
                        break;

                    case 'note_rechargeModifyTime':
                        $showStr = date('m-d H:i:s', $value[$key1]);
                        break;
                    case 'note_problemModifyTime':
                        $showStr = date('m-d H:i:s', $value[$key1]);
                        break;
                    case 'note_stopModifyTime':
                        $showStr = date('m-d H:i:s', $value[$key1]);
                        break;
                    case 'note_processModifyTime':
                        $showStr = date('m-d H:i:s', $value[$key1]);
                        break;
                    default:
                        $showStr = $value[$key1];
                        break;
                }
                $note1Array[$key][$value1] = $showStr;
            }
        }
        $noteArray = array();
        foreach ($note1Array as $key => $value) {
            foreach ($value as $key1 => $value1) {
                if($value['recharge'] == 1){
                    $noteArray['儲值記事'] = [
                        '會員帳號' => $value['memberAccount'],
                        '末碼' => $value['rechargeLastCode'],
                        '儲值地' => $value['rechargeArea'],
                        '備註' => $value['rechargeRemark'],
                        '修改時間' => $value['rechargeModifyTime'],
                    ];
                }
                if($value['problem'] == 1){
                    $noteArray['處理記事'] = [
                        '會員帳號' => $value['memberAccount'],
                        '末碼' => $value['problemAccount'],
                        '儲值地' => $value['problemRemark'],
                        '備註' => $value['problemProcess'],
                        '修改時間' => $value['problemModifyTime'],
                    ];
                }
                if($value['stop'] == 1){
                    $noteArray['問題記事'] = [
                        '會員帳號' => $value['memberAccount'],
                        '停權理由' => $value['stopStatus'],
                        '備註' => $value['stopRemark'],
                        '修改時間' => $value['stopModifyTime'],
                    ];
                }
                if($value['process'] == 1){
                    $noteArray['停權記事'] = [
                        '會員帳號' => $value['memberAccount'],
                        '對應帳號' => $value['processAccount'],
                        '處理方式' => $value['processMain'],
                        '處理備註1' => $value['processRemark1'],
                        '處理備註2' => $value['processRemark2'],
                        '修改時間' => $value['processModifyTime'],
                    ];
                }
            }
        }

        $this->assign("memberArray",$memberArray);//會員基本資料
        $this->assign("memberAccount",$memberArray[0]['account']);//會員帳號
        $this->assign("searchMemberId",$memberArray[0]['id']);//會員ID
        $this->assign("lastRechargeTime",$lastRechargeTime);//最後儲值時間
        $this->assign("bankdata",$bankdata);//會員銀行資料
        $this->assign("passwordStatus",$passwordStatus);//會員密碼資料
        $this->assign("memberEmpty","<tr><td colspan='12' align='center'>無會員基本資料</td></tr>");
        $this->assign("mailArray",$mailArray);//會員基本資料
        $this->assign("mailEmpty","<tr><td colspan='6' align='center'>無未回覆信件</td></tr>");
        $this->assign("withdrawArray",$withdrawArray);//會員託售紀錄
        $this->assign("withdrawEmpty","<tr><td colspan='8' align='center'>無託售紀錄</td></tr>");
        $this->assign("rechargeArray",$rechargeArray);//會員儲值紀錄
        $this->assign("rechargeEmpty","<tr><td colspan='9' align='center'>無儲值紀錄</td></tr>");
        $this->assign("loginArray",$loginArray);//會員登入紀錄
        $this->assign("loginEmpty","<tr><td colspan='6' align='center'>無登入紀錄</td></tr>");
        $this->assign("eventArray",$eventArray);//會員登入紀錄
        $this->assign("eventEmpty","<tr><td colspan='10' align='center'>無活動紀錄</td></tr>");
        $this->assign("noteArray",$noteArray);//工作記事
        $this->assign("notetEmpty","<div align='center'>無工作記事</div>");
		$this->display();
	}
